server {
    listen 8002;
	server_name localhost;
    host 127.0.0.1;
    root docs/fusion_web/;
    error_page 404 error_pages/404.html;

    location / {
        allow_methods  DELETE POST GET;
        autoindex off;
    }

    location /cgi-bin {
        root ./;
        allow_methods GET POST DELETE;
        index time.py;
        cgi_pass .php /usr/bin/php-cgi;
    }
}